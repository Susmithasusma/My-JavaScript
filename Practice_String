//check Ispalidrome true or false

function isPalindrome(str) {
    let reverse = "";
    for (let i = str.length - 1; i >= 0; i--) {
        reverse = reverse + str[i];
    }
    if (reverse === str) {
        return true;
    } else {
        return false;
    }
}

console.log(isPalindrome("radar")); // Output: true



